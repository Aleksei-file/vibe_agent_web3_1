    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.56.0",
    "less": "^4.2.0",
    "prettier": "^3.2.0",
    "typescript": "^5.2.0",
    "vite": "^5.1.0"
